—Ç–∞—Ü–∏–∏ uint32_t execution_count; // –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–π uint32_t last_modified; // –ü–æ—Å–ª–µ–¥–Ω–µ–µ –∏–∑–º–µ–Ω–µ–Ω–∏–µ uint32_t flags; // –§–ª–∞–≥–∏ (–∞—Ä—Ö–∏–≤–Ω—ã–π, –∞–∫—Ç–∏–≤–Ω—ã–π –∏ —Ç.–¥.) } evolutionary_gene_t; // –ü–æ–ª–Ω—ã–π –≥–µ–Ω–æ–º typedef struct { uint32_t genome_version; uint32_t current_generation; uint32_t total_mutations; uint32_t successful_mutations; uint32_t last_checkpoint_time; evolutionary_gene_t genes[MAX_GENES]; } evolution_genome_t; evolution_fs_header_t fs_header; evolution_genome_t current_genome; evolution_genome_t backup_genome; // –ê—Ç–æ–º–∞—Ä–Ω–æ–µ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è void atomic_genome_save() { // 1. –°–æ–∑–¥–∞–µ–º —Å–Ω–∏–º–æ–∫ —Ç–µ–∫—É—â–µ–≥–æ —Å–æ—Å—Ç–æ—è–Ω–∏—è evolution_genome_t snapshot = current_genome; snapshot.genome_version++; // 2. –ó–∞–ø–∏—Å—ã–≤–∞–µ–º –≤ —Ä–µ–∑–µ—Ä–≤–Ω—É—é –æ–±–ª–∞—Å—Ç—å if (write_to_disk(&snapshot, fs_header.backup_offset)) { // 3. –°–∏–Ω—Ö—Ä–æ–Ω–∏–∑–∏—Ä—É–µ–º —Å –¥–∏—Å–∫–æ–º (–≤–∞–∂–Ω–æ –¥–ª—è –∑–∞—â–∏—Ç—ã –æ—Ç –æ—Ç–∫–ª—é—á–µ–Ω–∏—è –ø–∏—Ç–∞–Ω–∏—è) disk_sync(); // 4. –û–±–Ω–æ–≤–ª—è–µ–º –∑–∞–≥–æ–ª–æ–≤–æ–∫ –∞—Ç–æ–º–∞—Ä–Ω–æ fs_header.generation = snapshot.current_generation; fs_header.checksum = calculate_checksum(&fs_header); // 5. –ó–∞–ø–∏—Å—ã–≤–∞–µ–º –æ–±–Ω–æ–≤–ª–µ–Ω–Ω—ã–π –∑–∞–≥–æ–ª–æ–≤–æ–∫ write_header_to_disk(&fs_header); // 6. –°–∏–Ω—Ö—Ä–æ–Ω–∏–∑–∏—Ä—É–µ–º —Å–Ω–æ–≤–∞ disk_sync(); // 7. –¢–æ–ª—å–∫–æ –ø–æ—Å–ª–µ —É—Å–ø–µ—à–Ω–æ–≥–æ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –∑–∞–≥–æ–ª–æ–≤–∫–∞ –∫–æ–ø–∏—Ä—É–µ–º –≤ –æ—Å–Ω–æ–≤–Ω—É—é –æ–±–ª–∞—Å—Ç—å copy_backup_to_primary(); disk_sync(); } } // –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–æ—Å–ª–µ —Å–±–æ—è –ø–∏—Ç–∞–Ω–∏—è void recovery_after_power_failure() { // 1. –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ü–µ–ª–æ—Å—Ç–Ω–æ—Å—Ç—å –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –∑–∞–≥–æ–ª–æ–≤–∫–∞ if (!verify_header_checksum(&fs_header)) { // 2. –ü—ã—Ç–∞–µ–º—Å—è –∑–∞–≥—Ä—É–∑–∏—Ç—å —Ä–µ–∑–µ—Ä–≤–Ω—É—é –∫–æ–ø–∏—é –∑–∞–≥–æ–ª–æ–≤–∫–∞ if (load_backup_header()) { // 3. –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –≥–µ–Ω–æ–º –∏–∑ —Ä–µ–∑–µ—Ä–≤–Ω–æ–π –∫–æ–ø–∏–∏ load_genome(&backup_genome, fs_header.backup_offset); current_genome = backup_genome; // 4. –§–∏–∫—Å–∏—Ä—É–µ–º –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ fs_header.generation = backup_genome.current_generation; write_header_to_disk(&fs_header); disk_sync(); } else { // 5. –ü–æ–ª–Ω–æ–µ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–∑ –∑–∞–≤–æ–¥—Å–∫–∏—Ö –Ω–∞—Å—Ç—Ä–æ–µ–∫ init_factory_defaults(); } } // 6. –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ü–µ–ª–æ—Å—Ç–Ω–æ—Å—Ç—å –≥–µ–Ω–æ–º–∞ if (!verify_genome_integrity(&current_genome)) { if (verify_genome_integrity(&backup_genome)) { current_genome = backup_genome; } else { init_factory_defaults(); } } } // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è EvolutionFS void init_evolution_fs() { // 1. –ü—ã—Ç–∞–µ–º—Å—è –∑–∞–≥—Ä—É–∑–∏—Ç—å —Ñ–∞–π–ª–æ–≤—É—é —Å–∏—Å—Ç–µ–º—É if (load_fs_header(&fs_header)) { // 2. –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–æ—Å–ª–µ –≤–æ–∑–º–æ–∂–Ω–æ–≥–æ —Å–±–æ—è recovery_after_power_failure(); // 3. –ó–∞–≥—Ä—É–∑–∫–∞ —Ç–µ–∫—É—â–µ–≥–æ –≥–µ–Ω–æ–º–∞ load_genome(&current_genome, fs_header.genome_offset); load_genome(&backup_genome, fs_header.backup_offset); } else { // 4. –°–æ–∑–¥–∞–Ω–∏–µ –Ω–æ–≤–æ–π —Ñ–∞–π–ª–æ–≤–æ–π —Å–∏—Å—Ç–µ–º—ã create_new_evolution_fs(); } // 5. –í—ã–≤–æ–¥ —Å—Ç–∞—Ç—É—Å–∞ –≤ VGA volatile uint16_t* vga = (volatile uint16_t*)0xB8000; const char* msg = "EvolutionFS: READY"; for (int i = 0; msg[i]; i++) { vga[80*2 + i*2] = msg[i]; vga[80*2 + i*2 + 1] = 0x0A; // –ó–µ–ª–µ–Ω—ã–π } } "@ | Out-File -FilePath "$buildDir\evolution_fs.c" -Encoding ASCII } function New-PersistentKernel { Write-Status "–°–æ–∑–¥–∞–Ω–∏–µ —è–¥—Ä–∞ —Å –ø–µ—Ä—Å–∏—Å—Ç–µ–Ω—Ç–Ω–æ–π —ç–≤–æ–ª—é—Ü–∏–µ–π..." "Cyan" @" // EvolutionOS ISO Kernel v2.0 - –ü–ï–†–°–ò–°–¢–ï–ù–¢–ù–ê–Ø –≠–í–û–õ–Æ–¶–ò–û–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê typedef unsigned int uint32_t; typedef unsigned char uint8_t; typedef unsigned long long uint64_t; extern void init_evolution_fs(); extern void atomic_genome_save(); extern void recovery_after_power_failure(); // –¢–∞–π–º–µ—Ä –¥–ª—è –ø–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–æ–≥–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è static uint32_t last_save_time = 0; static uint32_t save_interval = 1000000; // –ö–∞–∂–¥—ã–µ 1M —Ç–∞–∫—Ç–æ–≤ // –¢–µ—Å—Ç–æ–≤–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è —ç–≤–æ–ª—é—Ü–∏–∏ void test_function() { asm volatile( "pushl %ebp;" "movl %esp, %ebp;" "movl $0, %eax;" "movl $100, %ecx;" ".loop:" "incl %eax;" "decl %ecx;" "jnz .loop;" "popl %ebp;" "ret;" ); } // –≠–≤–æ–ª—é—Ü–∏–æ–Ω–Ω—ã–π —Ü–∏–∫–ª —Å –ø–µ—Ä—Å–∏—Å—Ç–µ–Ω—Ç–Ω–æ—Å—Ç—å—é void evolution_cycle() { static uint32_t generation = 0; uint32_t current_time = rdtsc(); // –≠–≤–æ–ª—é—Ü–∏–æ–Ω–Ω–∞—è –ª–æ–≥–∏–∫–∞ –∑–¥–µ—Å—å (—É–ø—Ä–æ—â–µ–Ω–æ) generation++; // –ü–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–æ–µ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è if (current_time - last_save_time > save_interval) { atomic_genome_save(); last_save_time = current_time; // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç—É—Å–∞ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è volatile uint16_t* vga = (volatile uint16_t*)0xB8000 + 80*3; const char* msg = "AUTO-SAVE: COMPLETE"; for (int i = 0; msg[i]; i++) { vga[i*2] = msg[i]; vga[i*2 + 1] = 0x0B; // –ì–æ–ª—É–±–æ–π } } // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—á–µ—Ç—á–∏–∫–∞ –ø–æ–∫–æ–ª–µ–Ω–∏–π volatile uint16_t* vga = (volatile uint16_t*)0xB8000; vga[80] = 'G'; vga[81] = 0x0E; vga[82] = 'e'; vga[83] = 0x0E; vga[84] = 'n'; vga[85] = 0x0E; vga[86] = ':'; vga[87] = 0x0E; vga[88] = '0' + (generation / 100) % 10; vga[89] = 0x0E; vga[90] = '0' + (generation / 10) % 10; vga[91] = 0x0E; vga[92] = '0' + generation % 10; vga[93] = 0x0E; } // –ö–æ—Ä—Ä–µ–∫—Ç–Ω–æ–µ –≤—ã–∫–ª—é—á–µ–Ω–∏–µ —Å–∏—Å—Ç–µ–º—ã void safe_shutdown() { // 1. –°–æ—Ö—Ä–∞–Ω—è–µ–º —Ç–µ–∫—É—â–µ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ atomic_genome_save(); // 2. –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –æ –≤—ã–∫–ª—é—á–µ–Ω–∏–∏ volatile uint16_t* vga = (volatile uint16_t*)0xB8000; const char* msg = "SYSTEM SHUTDOWN - STATE SAVED"; for (int i = 0; msg[i]; i++) { vga[80*10 + i*2] = msg[i]; vga[80*10 + i*2 + 1] = 0x0C; // –ö—Ä–∞—Å–Ω—ã–π } // 3. –û–∂–∏–¥–∞–Ω–∏–µ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è –æ—Ç –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è // (–í —Ä–µ–∞–ª—å–Ω–æ–π —Å–∏—Å—Ç–µ–º–µ –∑–¥–µ—Å—å –±—ã–ª –±—ã ACPI shutdown) while (1) { asm volatile("hlt"); } } // –û–±—Ä–∞–±–æ—Ç–∫–∞ –ø—Ä–µ—Ä—ã–≤–∞–Ω–∏—è —Ç–∞–π–º–µ—Ä–∞ (–¥–ª—è –ø–µ—Ä–∏–æ–¥–∏—á–µ—Å–∫–æ–≥–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è) void timer_interrupt_handler() { static uint32_t tick_count = 0; tick_count++; // –ö–∞–∂–¥—ã–µ 100 —Ç–∏–∫–æ–≤ - —Å–æ—Ö—Ä–∞–Ω—è–µ–º —Å–æ—Å—Ç–æ—è–Ω–∏–µ if (tick_count % 100 == 0) { atomic_genome_save(); } } // –ì–ª–∞–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è void main() { volatile uint16_t* vga = (volatile uint16_t*)0xB8000; const char* title = "EVOLUTION OS v2.0 - PERSISTENT AI CORE"; for (int i = 0; title[i]; i++) { vga[i*2] = title[i]; vga[i*2 + 1] = 0x1F; // –ë–µ–ª—ã–π –Ω–∞ —Å–∏–Ω–µ–º } const char* subtitle = "State preserved across power failures"; for (int i = 0; subtitle[i]; i++) { vga[80 + i*2] = subtitle[i]; vga[80 + i*2 + 1] = 0x0A; // –ó–µ–ª–µ–Ω—ã–π } // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Ñ–∞–π–ª–æ–≤–æ–π —Å–∏—Å—Ç–µ–º—ã init_evolution_fs(); // –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ –ø–æ—Å–ª–µ –≤–æ–∑–º–æ–∂–Ω–æ–≥–æ —Å–±–æ—è recovery_after_power_failure(); uint32_t last_input_time = rdtsc(); while (1) { evolution_cycle(); // –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã –¥–ª—è –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ–≥–æ –≤—ã–∫–ª—é—á–µ–Ω–∏—è if (check_key_pressed(0x01)) { // Esc safe_shutdown(); } // –ó–∞–¥–µ—Ä–∂–∫–∞ –¥–ª—è —Å–Ω–∏–∂–µ–Ω–∏—è –Ω–∞–≥—Ä—É–∑–∫–∏ for (volatile int i = 0; i < 5000; i++); } } "@ | Out-File -FilePath "$buildDir\kernel.c" -Encoding ASCII } function Build-Bootloader { Write-Status "–ö–æ–º–ø–∏–ª—è—Ü–∏—è –∑–∞–≥—Ä—É–∑—á–∏–∫–∞..." "White" # BIOS –∑–∞–≥—Ä—É–∑—á–∏–∫ nasm -f bin "$isoRoot\boot\boot.asm" -o "$isoRoot\boot\boot.bin" if ($LASTEXITCODE -ne 0) { throw "–û—à–∏–±–∫–∞ –∫–æ–º–ø–∏–ª—è—Ü–∏–∏ BIOS –∑–∞–≥—Ä—É–∑—á–∏–∫–∞" } # UEFI –∑–∞–≥—Ä—É–∑—á–∏–∫ (—É–ø—Ä–æ—â–µ–Ω–Ω–æ) if (Get-Command nasm -ErrorAction SilentlyContinue) { nasm -f elf64 "$isoRoot\boot\efi_boot.asm" -o "$isoRoot\boot\efi_boot.o" } } function Build-Kernel { Write-Status "–ö–æ–º–ø–∏–ª—è—Ü–∏—è —è–¥—Ä–∞..." "White" # –ö–æ–º–ø–∏–ª—è—Ü–∏—è EvolutionFS gcc -ffreestanding -nostdlib -m32 -c "$buildDir\evolution_fs.c" -o "$buildDir\evolution_fs.o" -O1 if ($LASTEXITCODE -ne 0) { throw "–û—à–∏–±–∫–∞ –∫–æ–º–ø–∏–ª—è—Ü–∏–∏ EvolutionFS" } # –ö–æ–º–ø–∏–ª—è—Ü–∏—è —è–¥—Ä–∞ gcc -ffreestanding -nostdlib -m32 -c "$buildDir\kernel.c" -o "$buildDir\kernel.o" -O1 if ($LASTEXITCODE -ne 0) { throw "–û—à–∏–±–∫–∞ –∫–æ–º–ø–∏–ª—è—Ü–∏–∏ —è–¥—Ä–∞" } # –õ–∏–Ω–∫–æ–≤–∫–∞ —è–¥—Ä–∞ ld -m i386pe -Ttext 0x10000 -o "$isoRoot\boot\kernel.bin" ` "$buildDir\kernel.o" ` "$buildDir\evolution_fs.o" ` --oformat binary if ($LASTEXITCODE -ne 0) { throw "–û—à–∏–±–∫–∞ –ª–∏–Ω–∫–æ–≤–∫–∏ —è–¥—Ä–∞" } } function Create-BootableISO { Write-Status "–°–æ–∑–¥–∞–Ω–∏–µ –∑–∞–≥—Ä—É–∑–æ—á–Ω–æ–≥–æ ISO-–æ–±—Ä–∞–∑–∞..." "Magenta" # –°–æ–∑–¥–∞–Ω–∏–µ –æ–±—Ä–∞–∑–∞ –¥–∏—Å–∫–∞ —Å —Ñ–∞–π–ª–æ–≤–æ–π —Å–∏—Å—Ç–µ–º–æ–π EvolutionFS $diskSize = 10 * 1024 * 1024 # 10MB $diskImage = "$buildDir\evolution_disk.img" # –°–æ–∑–¥–∞–Ω–∏–µ –ø—É—Å—Ç–æ–≥–æ —Ñ–∞–π–ª–∞ fsutil file createnew $diskImage $diskSize # –ó–∞–ø–æ–ª–Ω–µ–Ω–∏–µ –Ω–∞—á–∞–ª—å–Ω—ã–º–∏ –¥–∞–Ω–Ω—ã–º–∏ $header = New-Object byte[] 512 $header[0] = 0xE1 $header[1] = 0x01 $header[2] = 0x5F $header[3] = 0x5E [System.IO.File]::WriteAllBytes($diskImage, $header) # –°–æ–∑–¥–∞–Ω–∏–µ ISO-–æ–±—Ä–∞–∑–∞ if (Get-Command oscdimg -ErrorAction SilentlyContinue) { Write-Status "–ò—Å–ø–æ–ª—å–∑—É–µ–º oscdimg –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ISO..." "White" oscdimg -b"$isoRoot\boot\boot.bin" -h -u2 -m -lEVOLUTION_OS "$isoRoot" "$OutputPath" if ($LASTEXITCODE -ne 0) { Write-Status "oscdimg –∑–∞–≤–µ—Ä—à–∏–ª—Å—è —Å –æ—à–∏–±–∫–æ–π, –ø—Ä–æ–±—É–µ–º –∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –º–µ—Ç–æ–¥..." "Yellow" Create-FallbackISO } } else { Create-FallbackISO } Write-Status "ISO-–æ–±—Ä–∞–∑ —Å–æ–∑–¥–∞–Ω: $OutputPath" "Green" } function Create-FallbackISO { # –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –º–µ—Ç–æ–¥ —Å–æ–∑–¥–∞–Ω–∏—è ISO Write-Status "–°–æ–∑–¥–∞–Ω–∏–µ ISO –∞–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–º –º–µ—Ç–æ–¥–æ–º..." "Yellow" $tempIso = "$buildDir\temp.iso" # –°–æ–∑–¥–∞–Ω–∏–µ ISO —Å –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ–º mkisofs (–µ—Å–ª–∏ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω) if (Get-Command mkisofs -ErrorAction SilentlyContinue) { mkisofs -o $tempIso -b boot/boot.bin -no-emul-boot -boot-load-size 4 -boot-info-table -J -R -V "EVOLUTION_OS" $isoRoot } else { # –°–æ–∑–¥–∞–Ω–∏–µ –ø—Ä–æ—Å—Ç–æ–≥–æ –æ–±—Ä–∞–∑–∞ —Å –∑–∞–≥—Ä—É–∑—á–∏–∫–æ–º $isoContent = New-Object byte[] (4*1024*1024) # 4MB $bootBin = [System.IO.File]::ReadAllBytes("$isoRoot\boot\boot.bin") $kernelBin = [System.IO.File]::ReadAllBytes("$isoRoot\boot\kernel.bin") [System.Buffer]::BlockCopy($bootBin, 0, $isoContent, 0, $bootBin.Length) [System.Buffer]::BlockCopy($kernelBin, 0, $isoContent, 512, $kernelBin.Length) [System.IO.File]::WriteAllBytes($tempIso, $isoContent) } if (Test-Path $tempIso) { Move-Item $tempIso $OutputPath -Force } } function Test-ISOImage { Write-Status "–¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ ISO-–æ–±—Ä–∞–∑–∞ –≤ QEMU..." "Cyan" if (Get-Command qemu-system-x86_64 -ErrorAction SilentlyContinue) { Write-Host "–ó–∞–ø—É—Å–∫ —Ç–µ—Å—Ç–∞ –≤ QEMU..." Write-Host "–î–ª—è –≤—ã—Ö–æ–¥–∞: –∑–∞–∫—Ä–æ–π—Ç–µ –æ–∫–Ω–æ QEMU –∏–ª–∏ –Ω–∞–∂–º–∏—Ç–µ Ctrl+C" Write-Host "–î–ª—è –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ–≥–æ –≤—ã–∫–ª—é—á–µ–Ω–∏—è: –Ω–∞–∂–º–∏—Ç–µ ESC –≤ –æ–∫–Ω–µ QEMU" qemu-system-x86_64 ` -cdrom "$OutputPath" ` -boot d ` -m 64M ` -display sdl ` -no-reboot ` -machine accel=tcg Write-Status "–¢–µ—Å—Ç –∑–∞–≤–µ—Ä—à–µ–Ω —É—Å–ø–µ—à–Ω–æ!" "Green" } else { Write-Status "QEMU –Ω–µ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω. –ü—Ä–æ–ø—É—Å–∫–∞–µ–º —Ç–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ." "Yellow" Write-Status "–í—ã –º–æ–∂–µ—Ç–µ –ø—Ä–æ—Ç–µ—Å—Ç–∏—Ä–æ–≤–∞—Ç—å ISO –≤—Ä—É—á–Ω—É—é:" "White" Write-Status " qemu-system-x86_64 -cdrom ""$OutputPath"" -boot d -m 64M" "White" } } function Main { Write-Host "üåü EVOLUTION OS - –°–û–ó–î–ê–ù–ò–ï –ü–ï–†–°–ò–°–¢–ï–ù–¢–ù–û–ì–û ISO-–û–ë–†–ê–ó–ê" -ForegroundColor Cyan Write-Host "=================================================================" -ForegroundColor Cyan # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–∞–≤ –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä–∞ if (!(Test-Admin)) { Write-Host "‚ö†Ô∏è –†–µ–∫–æ–º–µ–Ω–¥—É–µ—Ç—Å—è –∑–∞–ø—É—Å–∫ –æ—Ç –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä–∞ –¥–ª—è —É—Å—Ç–∞–Ω–æ–≤–∫–∏ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–µ–π" -ForegroundColor Yellow } # –°–æ–∑–¥–∞–Ω–∏–µ –¥–∏—Ä–µ–∫—Ç–æ—Ä–∏–π if (!(Test-Path $projectDir)) { New-Item -ItemType Directory -Path $projectDir -Force New-Item -ItemType Directory -Path $buildDir -Force } Set-Location $projectDir # –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–µ–π if (!$SkipDependencies) { Install-Dependencies } # –°–æ–∑–¥–∞–Ω–∏–µ —Å—Ç—Ä—É–∫—Ç—É—Ä—ã ISO New-ISOStructure # –°–æ–∑–¥–∞–Ω–∏–µ EvolutionFS New-EvolutionFS # –°–æ–∑–¥–∞–Ω–∏–µ —è–¥—Ä–∞ New-PersistentKernel # –°–±–æ—Ä–∫–∞ Build-Bootloader Build-Kernel # –°–æ–∑–¥–∞–Ω–∏–µ ISO Create-BootableISO # –¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ Test-ISOImage Write-Host "`n‚úÖ –ò–°–û-–û–ë–†–ê–ó –ì–û–¢–û–í!" -ForegroundColor Green Write-Host " –§–∞–π–ª: $OutputPath" -ForegroundColor White Write-Host "`nüöÄ –ò–ù–°–¢–†–£–ö–¶–ò–ò –ü–û –ó–ê–ü–£–°–ö–£:" -ForegroundColor Magenta Write-Host " –í–∏—Ä—Ç—É–∞–ª—å–Ω–∞—è –º–∞—à–∏–Ω–∞ (VirtualBox/VMware):" -ForegroundColor Yellow Write-Host " 1. –°–æ–∑–¥–∞–π—Ç–µ –Ω–æ–≤—É—é –≤–∏—Ä—Ç—É–∞–ª—å–Ω—É—é –º–∞—à–∏–Ω—É" -ForegroundColor White Write-Host " 2. –í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –û–°: 'Other' -> 'Other/Unknown 32-bit'" -ForegroundColor White Write-Host " 3. –ü–æ–¥–∫–ª—é—á–∏—Ç–µ ISO-–æ–±—Ä–∞–∑ –∫–∞–∫ –∑–∞–≥—Ä—É–∑–æ—á–Ω—ã–π –¥–∏—Å–∫" -ForegroundColor White Write-Host " 4. –ó–∞–ø—É—Å—Ç–∏—Ç–µ –≤–∏—Ä—Ç—É–∞–ª—å–Ω—É—é –º–∞—à–∏–Ω—É" -ForegroundColor White Write-Host "`n –§–∏–∑–∏—á–µ—Å–∫–∏–π –∫–æ–º–ø—å—é—Ç–µ—Ä (—á–µ—Ä–µ–∑ USB):" -ForegroundColor Yellow Write-Host " 1. –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ Rufus –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –∑–∞–≥—Ä—É–∑–æ—á–Ω–æ–π USB-—Ñ–ª–µ—à–∫–∏" -ForegroundColor White Write-Host " 2. –í—ã–±–µ—Ä–∏—Ç–µ: 'ISO Image mode', 'MBR partition scheme', 'BIOS or UEFI'" -ForegroundColor White Write-Host " 3. –ó–∞–≥—Ä—É–∑–∏—Ç–µ—Å—å —Å USB-—Ñ–ª–µ—à–∫–∏" -ForegroundColor White Write-Host "`n –¢–µ—Å—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ –≤ QEMU:" -ForegroundColor Yellow Write-Host " qemu-system-x86_64 -cdrom ""$OutputPat